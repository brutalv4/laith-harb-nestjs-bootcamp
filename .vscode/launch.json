  "version": "0.2.0",
  "configurations": [
    {
      "name": "expense-app",
      "type": "node",
      "request": "launch",
      "console": "internalConsole",
      "cwd": "${workspaceFolder}/expense-app",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "start:debug"
      ],
      "env": {
        "PORT": "3001",
      },
      "skipFiles": [
        "<node_internals>/**",
        "**/node_modules/**"
      ],
    }
  ]
}